//A. Blackjack
#include <iostream>
#include <cmath>
#include <string>
#include <vector>
#include <set>
#include <algorithm>
#include <cctype> 
#include <iomanip>
#include <initializer_list>

using namespace std;
#define lll long long
	int main() {
		lll x;
		cin >> x;
		int first = x - 10;
		int scores_ofcard[] = { 1,11,2, 3, 4, 5, 6, 7, 8, 9, 10,10,10,10 };
		int counter = 0;
		for (int i = 0;i<14;i++) {
			if(first == scores_ofcard[i]) {
				counter++;
				
			}
			

		}
		if (first == 10) {
			cout << (counter * 4)-1;

		}
		else {
			cout << counter * 4;

		}
		
		

		return 0;
		
	}
